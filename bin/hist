#!/usr/bin/env python
# -*- coding: utf-8 -*- 
import sys, os
from bashplotlib.utils.commandhelp import HIST_DOCSTRING as __doc__
from bashplotlib.histogram import plot_hist
from bashplotlib.utils.commandhelp import parse_args

def main():
    plot_params = parse_args(__doc__)
    plot_hist(**plot_params)

if __name__ == "__main__":
    main()
