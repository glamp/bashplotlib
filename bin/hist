#!/usr/bin/env python
# -*- coding: utf-8 -*- 
import os, sys
from bashplotlib.cli.hist import __doc__
from bashplotlib.cli.hist import parse_args
from bashplotlib.core.histogram import plot_hist


def main():
    plot_params = parse_args()
    plot_hist(**plot_params)

if __name__ == "__main__":
    main()
