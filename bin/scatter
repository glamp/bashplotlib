#!/usr/bin/env python
# -*- coding: utf-8 -*- 
import os
import sys
from bashplotlib.cli.scatter import __doc__
from bashplotlib.cli.scatter import parse_args
from bashplotlib.core.scatterplot import plot_scatter

def main():
    plot_params = parse_args()
    plot_scatter(**plot_params)

if __name__ == "__main__":
    main()
